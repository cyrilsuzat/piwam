<h2>Trombinoscope</h2>

<ul class="thumbnails clearfix">
  <% @members.each do |member| %>
    <li>
      <%= link_to image_tag(member.photo.url), member %>
      <div class="caption" title="<%= member %>"><%= truncate member.to_s, length: 18 %></div>
    </li>
  <% end %>
</div>

<%= paginate @members %>